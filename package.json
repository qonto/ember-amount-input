{
  "name": "ember-amount-input",
  "version": "7.3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qonto/ember-amount-input.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm --filter ember-amount-input build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "lint:types": "pnpm --filter '*' lint:types",
    "postinstall": "lefthook install",
    "prepare": "pnpm build",
    "release": "release-it",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-amount-input start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@release-it-plugins/workspaces": "^4.2.0",
    "@release-it/conventional-changelog": "^9.0.1",
    "concurrently": "^8.2.2",
    "lefthook": "^1.5.5",
    "prettier": "^3.2.5",
    "release-it": "^17.6.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "volta": {
    "node": "18.15.0"
  },
  "packageManager": "pnpm@8.6.5",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
